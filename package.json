  "version": "0.3.1",
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "keywords": [
    "parse",
    "yaml",
    "yml",
    "json",
    "ast",
    "abstract",
    "syntax",
    "tree",
    "loc",
    "location"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fmvilas/pseudo-yaml-ast.git"
  },
  "main": "src/index.js",
  "scripts": {
